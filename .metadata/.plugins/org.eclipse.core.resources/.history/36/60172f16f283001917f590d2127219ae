package arrays;

public class MaximumSubArray {

	public static void main(String[] args) {
		
		
	}
	
	//Sample input [-2,1,-3,4,-1,2,1,-5,4]
	public int maxSubArray(int[] a) {
		int current_max = a[0];
		int max_soFar = a[0];
		for(int i =1; i< a.length; i++) {
			current_max = Math.max(a[i], a[i] + current_max);
		}
	}
}

